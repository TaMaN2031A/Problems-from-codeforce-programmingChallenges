#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
void solve()
{
    play n, a;
    cin >> n;
    for(int i = 0; i < n; i++)
        cin >> a;
    cout << a << endl;
 
}
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
