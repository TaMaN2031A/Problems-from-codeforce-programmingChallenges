#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long

using namespace std;
void solve()
{
    play n;
    cin >> n;
    if(n&1)
        cout << -1 << endl;
    else
        cout << 0 <<" " <<0 << " " << n/2 << endl;
}
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
