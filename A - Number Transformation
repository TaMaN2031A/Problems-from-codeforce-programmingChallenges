#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
 
void solve()
{
    int x, y;
    cin >> x >> y;
    if(y%x)
        cout << 0 << " " << 0 << endl;
    else
        cout << 1 << " " << y/x << endl;
 
}
 
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
