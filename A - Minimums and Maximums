#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
void solve()
{
    play l1, r1, l2, r2;
    cin >> l1 >> r1 >> l2 >> r2;
    if(max(l1, l2) <= min(r1,r2))
        cout << max(l1, l2) << endl;
    else
        cout << l1 + l2 << endl;
}
 
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
