#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long

using namespace std;
void solve()
{
    int n;
    cin >> n;
    if(n%2)
    {
        cout << 1 << " ";
        int he = 3;
        for(int i = 0; i < n/2; i++) {
            cout << he << " " << he - 1 << " ";
            he+=2;
        }
    }
    else
    {
        int he = 2;
        for(int i = 0; i < n/2; i++) {
            cout << he << " " << he - 1 << " ";
            he+=2;
        }
    }
    cout << endl;
}
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
