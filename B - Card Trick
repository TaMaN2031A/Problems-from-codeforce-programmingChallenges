#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
play arr[200000];
void solve()
{
    play n, m, k, c = 0;
    cin >> n;
    for(int i = 0; i < n; i++)
        cin >> arr[i];
    cin >> m;
    for(int i = 0; i < m; i++)
    {
        cin >> k;
        c += k;
    }
    cout << arr[c%n] << endl;
 
}
 
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
