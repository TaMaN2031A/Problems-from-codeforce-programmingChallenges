#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long

using namespace std;
void solve()
{
    int n, a, b, c = 0;
    cin >> n >> a;
    for(int i = 0; i < n; i++) {
        cin >> b;
        c+=b;
    }
    cout << max(c - a, 0) << endl;
}
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
