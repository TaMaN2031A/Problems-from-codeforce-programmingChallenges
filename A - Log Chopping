#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
 
void solve()
{
    int n, c = 0;
    cin >> n;
    int b;
    for(int i = 0; i < n; i++)
    {
 
        cin >> b;
        c+=b-1;
    }
    if(c&1)
        cout << "errorgorn" << endl;
    else
        cout <<  "maomao90"  << endl;
}
 
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
