#include <iostream>
#include <bits/stdc++.h>
#define fire cout
#define play long long
 
using namespace std;
void solve()
{
    play n;
    cin >> n;
    fire << 1 << " " << n-3 << " " << 1 << " " << 1 << endl;
}
 
int main() {
    play t;
    cin >> t;
    while(t--)
        solve();
    return 0;
}
